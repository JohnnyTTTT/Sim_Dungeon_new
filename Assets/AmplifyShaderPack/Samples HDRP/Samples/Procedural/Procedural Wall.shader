				float2 uv_InnerAlbedo = packedInput.ase_texcoord5.xy * _InnerAlbedo_ST.xy + _InnerAlbedo_ST.zw;
				float2 uv_BrickAlbedo = packedInput.ase_texcoord5.xy * _BrickAlbedo_ST.xy + _BrickAlbedo_ST.zw;
